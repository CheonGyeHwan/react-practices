<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kanbanboard">
	<select id="findAllToCard" resultType="cardvo">
		<![CDATA[
			SELECT no, title, description, status FROM card
		]]>
	</select>
	<select id="findAllToTask" resultType="taskvo">
		<![CDATA[
			SELECT no, name, done, card_no FROM task
		]]>
	</select>
</mapper>